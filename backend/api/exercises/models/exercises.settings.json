{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "name": "Exercises"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "tags": {
      "collection": "tags",
      "via": "exercises",
      "dominant": true
    },
    "Course_Downloadable_Materials": {
      "type": "dynamiczone",
      "components": [
        "files.course-material"
      ]
    },
    "exercises_related_to_this": {
      "collection": "exercises",
      "via": "related_exercises",
      "dominant": true
    },
    "related_exercises": {
      "collection": "exercises",
      "via": "exercises_related_to_this"
    },
    "Contents": {
      "type": "dynamiczone",
      "components": [
        "contents.musical-notation",
        "contents.rich-text-center-aligned",
        "contents.rich-text",
        "contents.video"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Summary": {
      "type": "text",
      "required": true
    },
    "Exercise_Content": {
      "type": "richtext"
    }
  }
}
